{
  "name": "markdownenglishwordhint",
  "displayName": "Markdown English Word Hint",
  "version": "0.0.1",
  "publisher": "jhamtan",
  "description": "Provide English word hint in Markdown file.",
  "author": {
    "name": "jhamtan"
  },
  "categories": ["Other"],
  "engines": {
    "vscode": "^1.79.0"
  },
  "bugs": {
    "url": "https://github.com/jhamtan/markdownenglishwordhint/issues",
    "email": "Jhamtan@outlook.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jhamtan/markdownenglishwordhint.git"
  },
  "homepage": "https://github.com/jhamtan/markdownenglishwordhint/blob/main/README.md",
  "activationEvents": ["onStartupFinished"],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": [
      {
        "title": "MarkdownEnglishWordHint",
        "properties": {
          "MarkdownEnglishWordHint.activation": {
            "type": "object",
            "default": {
              "html": false,
              "java": false,
              "javascript": false,
              "javascriptreact": false,
              "json": false,
              "typescript": false,
              "typescriptreact": false,
              "vue": false,
              "vue-html": false,
              "xml": false
            },
            "properties": {
              "html": {
                "type": "boolean",
                "default": false,
                "description": "Activate this extension in the HTML file."
              },
              "java": {
                "type": "boolean",
                "default": false,
                "description": "Activate this extension in the JAVA file."
              },
              "javascript": {
                "type": "boolean",
                "default": false,
                "description": "Activate this extension in the JavaScript file."
              },
              "javascriptreact": {
                "type": "boolean",
                "default": false,
                "description": "Activate this extension in the JavaScript JSX file."
              },
              "json": {
                "type": "boolean",
                "default": false,
                "description": "Activate this extension in the JSON file."
              },
              "typescript": {
                "type": "boolean",
                "default": false,
                "description": "Activate this extension in the TypeScript file."
              },
              "typescriptreact": {
                "type": "boolean",
                "default": false,
                "description": "Activate this extension in the TypeScript JSX file."
              },
              "vue": {
                "type": "boolean",
                "default": false,
                "description": "Activate this extension in the Vue file."
              },
              "vue-html": {
                "type": "boolean",
                "default": false,
                "description": "Activate this extension in the Vue HTML file."
              },
              "xml": {
                "type": "boolean",
                "default": false,
                "description": "Activate this extension in the XML file."
              }
            },
            "additionalProperties": false,
            "markdownDescription": "Configure the language for activating the extension."
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.79.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "ts-loader": "^9.4.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1",
    "@vscode/test-electron": "^2.3.2"
  }
}
